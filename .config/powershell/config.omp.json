{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "osc99": true,
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "diamond",
                    "leading_diamond": "",
                    "foreground": "#ffffff",
                    "background": "#cc3802",
                    "template": "{{ if .Env.PNPPSHOST }}  {{ .Env.PNPPSHOST }} {{ end }}"
                },
                {
                    "type": "text",
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "background": "#047e84",
                    "powerline_symbol": "",
                    "template": "{{ if .Env.PNPPSSITE }}  {{ .Env.PNPPSSITE }}{{ end }}"
                },
                {
                    "type": "text",
                    "style": "diamond",
                    "trailing_diamond": "",
                    "foreground": "#ffffff",
                    "background": "#047e84",
                    "template": "{{ if .Env.PNPPSSITE }} {{ end }}"
                }
            ],
            "type": "rprompt"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#125179",
                    "foreground": "#ffffff",
                    "style": "diamond",
                    "leading_diamond": "\u256d\u2500\ue0b6",
                    "template": "\uf120 {{ .Name }} ",
                    "type": "shell"
                },
                {
                    "background": "#ef5350",
                    "foreground": "#FFFB38",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "properties": {
                        "root_icon": "\uf292",
                        "prefix": "<parentBackground>\uE0B0</> "
                    },
                    "type": "root"
                },
                {
                    "background": "#f808f8",
                    "foreground": "#111111",
                    "properties": {
                        "folder_icon": " \uf07b ",
                        "home_icon": " \uf7dd ",
                        "style": "full"
                    },
                    "template": " \uf07b \uf553 {{ .Path }} ",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "type": "path"
                },
                {
                    "background": "cyan",
                    "foreground": "black",
                    "properties": {
                        "style": "austin",
                        "threshold": 0
                    },
                    "template": " {{ .FormattedMs }} ",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "type": "executiontime"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "template": "",
                    "style": "plain",
                    "trailing_diamond": "\uE0B0",
                    "type": "text"
                },
                {
                    "background": "#fff",
                    "foreground": "#fff",
                    "template": "\ufc62",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "type": "text"
                },
                {
                    "background": "#fa2424",
                    "foreground": "#fa2424",
                    "template": "\ufc62",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "type": "text"
                },
                {
                    "background": "#fff",
                    "foreground": "#fff",
                    "template": "\ufc62",
                    "style": "diamond",
                    "trailing_diamond": "\uE0B0",
                    "type": "text"
                },
                {
                    "background": "#185833",
                    "foreground": "#ffffff",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#f1184c{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "template": " \ufc8d ",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "type": "text"
                },
                {
                    "foreground": "#000000",
                    "background": "#d13bd1",
                    "powerline_symbol": "\uE0B0",
                    "style": "powerline",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
                        "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
                        "{{ if gt .Behind 0 }}#C792EA{{ end }}"
                    ],
                    "properties": {
                        "branch_icon": "\ue725 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_worktree_count": true,
                        "template": "{{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uF046 {{ .Staging.String }}</>{{ end }}"
                    },
                    "type": "git"
                },
                {
                    "background": "#316ce4",
                    "foreground": "#ffffff",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "template": "\ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
                    "type": "kubectl"
                },
                {
                    "background": "#2e3f61",
                    "foreground": "#ffffff",
                    "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
                    "powerline_symbol": "\uE0B0",
                    "style": "powerline",
                    "type": "dotnet"
                },
                {
                    "foreground": "#262626",
                    "background": "#f36943",
                    "background_templates": [
                        "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
                        "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
                        "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
                    ],
                    "properties": {
                        "charged_icon": "\uf58e ",
                        "charging_icon": "\uf1e6 ",
                        "discharging_icon": "\ue234 "
                    },
                    "powerline_symbol": "\uE0B0",
                    "style": "powerline",
                    "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} ",
                    "type": "battery"
                },
                {
                    "background": "#33658A",
                    "foreground": "#ffffff",
                    "properties": {
                        "time_format": "Mon \ue385 15:04:05"
                    },
                    "style": "diamond",
                    "trailing_diamond": "\ue0b4",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#125179",
                    "style": "plain",
                    "template": "\u2570\u2500",
                    "type": "text"
                },
                {
                    "foreground": "#e0f8ff",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#ef5350{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": "\ue285 ",
                    "type": "exit"
                }
            ]
        }
    ],
    "final_space": false,
    "version": 2
}
