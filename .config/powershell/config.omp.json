{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "osc99": true,
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "diamond",
                    "leading_diamond": "",
                    "foreground": "#ffffff",
                    "background": "#cc3802",
                    "template": "{{ if .Env.PNPPSHOST }}  {{ .Env.PNPPSHOST }} {{ end }}"
                },
                {
                    "type": "text",
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "background": "#047e84",
                    "powerline_symbol": "",
                    "template": "{{ if .Env.PNPPSSITE }}  {{ .Env.PNPPSSITE }}{{ end }}"
                },
                {
                    "type": "text",
                    "style": "diamond",
                    "trailing_diamond": "",
                    "foreground": "#ffffff",
                    "background": "#047e84",
                    "template": "{{ if .Env.PNPPSSITE }} {{ end }}"
                }
            ],
            "type": "rprompt"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#125179",
                    "foreground": "#ffffff",
                    "style": "diamond",
                    "leading_diamond": "\u256d\u2500\ue0b6",
                    "type": "shell"
                },
                {
                    "background": "#ef5350",
                    "foreground": "#FFFB38",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "properties": {
                        "root_icon": "\uf292",
                        "prefix": "<parentBackground>\uE0B0</> "
                    },
                    "type": "root"
                },
                {
                    "background": "#421470",
                    "foreground": "#ffffff",
                    "properties": {
                        "style": "full",
                        "enable_hyperlink": true
                    },
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "background": "#135c13",
                    "foreground": "#ffffff",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#f1184c{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "template": " \ufc8d ",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "type": "text"
                },
                {
                    "foreground": "#000000",
                    "background": "#acaa56",
                    "powerline_symbol": "\uE0B0",
                    "style": "powerline",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
                        "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
                        "{{ if gt .Behind 0 }}#C792EA{{ end }}"
                    ],
                    "properties": {
                        "branch_icon": "\ue725 ",
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "template": "{{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uF046 {{ .Staging.String }}</>{{ end }}"
                    },
                    "type": "git"
                },
                {
                    "background": "#33658A",
                    "foreground": "#ffffff",
                    "properties": {
                        "prefix": " \uf5ef ",
                        "postfix": " "
                    },
                    "style": "diamond",
                    "trailing_diamond": "\ue0b4",
                    "template": " \ue385 {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#125179",
                    "style": "plain",
                    "template": "\u2570\u2500",
                    "type": "text"
                },
                {
                    "foreground": "#e0f8ff",
                    "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
                    "properties": {
                      "always_enabled": true
                    },
                    "style": "plain",
                    "template": "\ue285 ",
                    "type": "exit"
                  }
            ]
        }
    ],
    "final_space": false,
    "version": 2
}
